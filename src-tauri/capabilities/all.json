{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview-window",
    "core:webview:allow-webview-hide",
    "core:webview:allow-webview-show",
    "core:window:allow-close",
    "dialog:allow-open",
    "fs:allow-copy-file",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-watch",
    "fs:allow-write-file",
    {
      "identifier": "fs:scope",
      "allow": ["**"]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://dl.smwcentral.net/*"
        },
        {
          "url": "https://www.smwcentral.net/*"
        }
      ]
    },
    "opener:default",
    "opener:allow-open-path",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        { "url": "https://zuccha.io" },
        { "url": "https://github.com/*" },
        { "url": "https://projects.sappharad.com/multipatch" },
        { "url": "https://www.smwcentral.net/*" }
      ]
    }
  ]
}
